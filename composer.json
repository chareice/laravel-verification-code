{
    "name": "chareice\/laravel-verification-code",
    "description": "Verification Code Generator",
    "license": "MIT",
    "authors": [
        {
            "name": "Chareice",
            "email": "chareice@live.com"
        }
    ],
    "require": {
        "laravel/framework": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Chareice\\VerificationCode\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chareice\\VerificationCode\\ServiceProvider"
            ]
        }
    }
}
